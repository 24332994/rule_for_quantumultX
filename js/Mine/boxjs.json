        {
          "id": "@njtechAutoLogin.njtech_pwd",
          "name": "密码",
          "val": null,
          "type": "text",
          "autoGrow": false,
          "rows": 5,
          "desc": "填写密码,必填"
        },
        {
          "id": "@njtechAutoLogin.njtech_option",
